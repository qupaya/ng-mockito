    "nx": "14.1.4",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "2.6.2",
    "semantic-release": "^17.2.3",
    "ts-jest": "27.1.4",
    "ts-mockito": "^2.6.1",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.6.4"
  }
