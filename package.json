    "@semantic-release/git": "^9.0.0",
    "@testing-library/angular": "^10.1.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "cypress": "^4.1.0",
